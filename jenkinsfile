              steps{  
                sh 'echo this is Build'
              }
           
            }
            stage('Deploy'){
                steps{
                  sh 'echo this is teast'
                  
                }
            }
            stage('Deploy'){
                steps{
                  sh 'echo this is Deploy'  
                }
            }
            

    }
    
        }

